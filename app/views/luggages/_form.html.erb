<%= simple_form_for(@luggage) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.association :trip %>
    <%= f.association :participant %>
    <%= f.input :quantity %>
    <%= f.input :price %>
    <%= f.input :comment %>
    <%= f.input :importance %>
    <%= f.input :taken_care_of %>
    <%= f.input :taken_care_of_by_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
